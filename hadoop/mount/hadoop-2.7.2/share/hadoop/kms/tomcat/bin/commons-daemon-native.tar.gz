	m_wndToolBar.SetOwner(this);

	// ВЈВ„ВƒВћГRГ}ГУГhВ™БAРeГtГМБ[ГАМoЧRВ≈ВЌВ»В≠В±ВћГRГУГgГНБ[ГЛМoЧRВ≈УnВ≥ВкВ№ВЈ:
	m_wndToolBar.SetRouteCommandsViaFrame(FALSE);

	CMenu menuSort;
